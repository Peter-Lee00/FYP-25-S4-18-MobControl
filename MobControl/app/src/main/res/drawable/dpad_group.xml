<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background circle -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#2A2A3A" />
            <stroke android:width="3dp" android:color="#1A1A2A" />
            <size android:width="180dp" android:height="180dp" />
        </shape>
    </item>
    
    <!-- Up arrow -->
    <item android:left="65dp" android:top="20dp">
        <shape android:shape="rectangle">
            <solid android:color="#424242" />
            <corners android:radius="8dp" />
            <size android:width="50dp" android:height="50dp" />
        </shape>
    </item>
    
    <!-- Down arrow -->
    <item android:left="65dp" android:top="110dp">
        <shape android:shape="rectangle">
            <solid android:color="#424242" />
            <corners android:radius="8dp" />
            <size android:width="50dp" android:height="50dp" />
        </shape>
    </item>
    
    <!-- Left arrow -->
    <item android:left="20dp" android:top="65dp">
        <shape android:shape="rectangle">
            <solid android:color="#424242" />
            <corners android:radius="8dp" />
            <size android:width="50dp" android:height="50dp" />
        </shape>
    </item>
    
    <!-- Right arrow -->
    <item android:left="110dp" android:top="65dp">
        <shape android:shape="rectangle">
            <solid android:color="#424242" />
            <corners android:radius="8dp" />
            <size android:width="50dp" android:height="50dp" />
        </shape>
    </item>
</layer-list>
